COMPONENT seeding task

RESOURCE db dynamodb -p hashKeyName=name -p hashKeyType=S -p rangeKeyName=type -p rangeKeyType=S
ENV DB_ENDPOINT $.resources.db.endpoint
ENV DB_TABLENAME $.resources.db.tableName

FROM node:alpine
COPY package*.json  ./
RUN npm install
COPY app.js .
ENTRYPOINT npm start
LIFECYCLE postdeploy
